{
  "name": "schema-form",
  "version": "0.1.0-0",
  "description": "An engine that generates forms based on JSON Schema",
  "repository": "https://github.com/Muluk-m/schema-form",
  "private": true,
  "workspaces": [
    "packages/vue3-example/**",
    "packages/vue3-schema-form/**"
  ],
  "scripts": {
    "start:vue3": "yarn --cwd packages/examples start",
    "build": "node scripts/build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js packages",
    "lint-stylelint": "stylelint --config .stylelintrc.js packages/**/src/*.{less,css,scss}",
    "fix": "npm run eslint 'packages/**/src/*.{js,ts,tsx}' --fix",
    "commit": "git-cz",
    "test": "jest",
    "test:c": "jest --coverage",
    "prepare": "husky install",
    "prettier": "prettier -w 'packages/**/src/*.{js,ts,tsx}'",
    "release": "node ./scripts/release",
    "clean": "rimraf packages/**/dist",
    "lint-staged": "lint-staged"
  },
  "keywords": [
    "json-schema-form",
    "schema-form"
  ],
  "author": {
    "name": "Muluk",
    "email": "maqiqiqan0316@163.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "unplugin-vue-jsx": "^0.1.1",
    "rollup-plugin-vue": "5.1.9",
    "commitizen": "^4.2.4",
    "consola": "^2.15.3",
    "cssnano": "^5.0.13",
    "cz-conventional-changelog": "^3.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.4.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^10.5.4",
    "postcss": "^8.3.6",
    "prettier": "^2.5.0",
    "rollup": "^2.60.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "sass": "^1.45.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.2.3",
    "unplugin-vue-components": "^0.19.6",
    "vant": "^3.5.1",
    "vue": "^3.2.2",
    "vue-tsc": "^0.34.7"
  }
}
